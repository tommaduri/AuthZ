{
  "name": "@authz-engine/nestjs",
  "version": "0.1.0",
  "description": "NestJS integration for Aegis Authorization Engine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@authz-engine/sdk": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependenciesMeta": {
    "@nestjs/graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^10.2.0",
    "@nestjs/core": "^10.2.0",
    "@types/node": "^20.10.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": ["authorization", "nestjs", "guards", "decorators"],
  "license": "MIT"
}
