{
  "name": "@authz-engine/agents",
  "version": "0.1.0",
  "description": "Agentic authorization framework - GUARDIAN, ANALYST, ADVISOR, ENFORCER",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "dev": "tsc --watch",
    "bench": "vitest bench benchmarks/",
    "bench:pipeline": "vitest bench benchmarks/agent-pipeline.bench.ts",
    "bench:throughput": "vitest bench benchmarks/throughput.bench.ts"
  },
  "dependencies": {
    "@authz-engine/core": "workspace:*",
    "eventemitter3": "^5.0.1",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "openai": "^4.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@authz-engine/core": "workspace:*"
  },
  "keywords": ["authorization", "agents", "anomaly-detection", "ai", "llm"],
  "license": "MIT"
}
